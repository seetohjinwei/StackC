def , dup . end

// FizzBuzz from 1 to 100
def FIZZ_MAX 100 end // using as a constant
1
while dup FIZZ_MAX <= then
  if dup 15 % 0 = then
    "FizzBuzz\n" .
  elseif dup 3 % 0 = then
    "Fizz\n" .
  elseif dup 5 % 0 = then
    "Buzz\n" .
  elseif 1 then
    , "\n" .
  end
  1 +
end
drop

// Simple 10 to 1 loop

10
while dup 0 > then
  ,
  1 -
end
drop

// Nested while loops

5
while dup 0 > then
  3
  while dup 0 > then
    ,
    1 -
  end
  drop
  1 -
end
drop

// "Hi" 5 times

5
while dup 0 > then
  "Hi\n" .
  1 -
end
drop
